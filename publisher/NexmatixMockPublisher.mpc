project: dcpsexe, svc_utils, openssl {
  exeout += ../bin

  idlflags += -I../idl

  Modify_Custom(IDL) {
    output_follows_input = 0
  }

  TypeSupport_Files {
    ../idl/Nexmatix.idl
  }

  IDL_Files {
    ../idl/Nexmatix.idl
    gendir = .
  }

  verbatim(gnuace, extension, 1) {
    LDLIBS += $(if $(findstring 1, $(VXWORKS)), -lunix)
  }
}
